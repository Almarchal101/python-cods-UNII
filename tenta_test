"""
def  doubled_odds(seq: list):
    result = seq.copy()
    
    if not seq:
        return []
    
  
    if isinstance(result[0],int) and result[0] % 2 != 0:
       
        return [result[0]* 2] + doubled_odds(seq[1:])
    
    if isinstance(result[0],list):
        return [doubled_odds(seq[0])] + doubled_odds(seq[1:])
    
    else:
        return  [result[0]] + doubled_odds(seq[1:]) 
    
 
print(doubled_odds([12, 'hu', 3]))
print(doubled_odds([-1, [2, 3], ['Hi', 4, [7]]]) )

assert doubled_odds([1, 2, 3]) == [2, 2, 6]
print(doubled_odds([-1, [2, 3], ('Hi', 4, [7])])) """


"""def sum_satisfying(fun, pred):
    
    def add(seq):
        total = 0
        for x in seq:
            if pred(x):
                total += fun(x)
        return total
    return add

print(sum_satisfying(len, str.isdigit)(['10','yes','no','whatever','4711']))
            


sum_square_negative_odd = sum_satisfying(lambda x: x * x, lambda x: isinstance(x, int) and x < 0 and x % 2 != 0)
print(sum_square_negative_odd([-1, 0, -3, 5, 2, 7]))"""


def count_rows(seq):
    return len(seq)

def count_column(seq):
    return len(seq[0])
            

print(count_rows([[1, 0, 2], [-1, 3, 1]]))
print(count_column([[1, 0, 2], [-1, 3, 1]]))
print(count_column([[1, -1], [0, 3], [2, 1]]))
print(count_rows([[1, -1], [0, 3], [2, 1]]))

def transpornat(seq):
    result = []
    
    for j in range(count_column(seq)):
        rows = []
        for i in range(count_rows(seq)):
            rows.append(seq[i][j])
        result.append(rows)
        
    return result

print(transpornat([[1, 0, 2], [-1, 3, 1]]))
        

def map(matix,fun):
    result = []
    for i in range(count_rows(matix)):
        new_row = []
        for j in range(count_column(matix)):
            new_row.append(fun(matix[i][j]))
        result.append(new_row)
        
    return result
         
         
print(map([[1, 0, 2], [-1, 3, 1]], lambda x: -x))

