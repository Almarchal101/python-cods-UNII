def split_lists(seq, sizes):
    
    def add(sizes):
        
        if not sizes:
            return 0
        else:
            return int(sizes[0]) + add(sizes[1:])
        
    if add(sizes) != len(seq):
        return (None, None)
    
    result = []
    
    if not seq:
        return []
    
    else:
        length = int(sizes[0])
        result.append(seq[:length])
        return result + split_lists(seq[length:], sizes[1:]) 
    
    
    
  
